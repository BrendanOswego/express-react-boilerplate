{
  "name": "generator-newr",
  "version": "0.0.1",
  "description": "React Boilerplate with Express (production) and Webpack Dev Server (development)",
  "repository": "https://github.com/BrendanOswego/generator-newr",
  "scripts": {
    "build": "webpack --progress --config ./webpack/prod.js",
    "dev": "webpack-dev-server --progress --config ./webpack/dev.js",
    "start": "node ./src/server/index.js",
    "test": "cross-env NODE_ENV=test jest --coverage --config='./configs/jest.config.js'",
    "report": "cross-env NODE_ENV=report jest --coverage --config='./configs/jest.config.js'"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.18",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.16.0",
    "eslint-plugin-jasmine": "^2.9.3",
    "eslint-plugin-react": "^7.6.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "identity-obj-proxy": "^3.0.0",
    "ignore-styles": "^5.0.1",
    "jasmine": "^3.1.0",
    "jasmine-console-reporter": "^2.0.1",
    "jest": "^22.4.2",
    "jsdom": "^11.6.2",
    "node-sass": "^4.7.2",
    "nyc": "^11.4.1",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-mock-store": "^1.5.1",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^6.0.6",
    "sinon": "^4.4.2",
    "style-loader": "^0.20.1",
    "uglifyjs-webpack-plugin": "^1.2.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
