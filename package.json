{
  "name": "express-react-boilerplate",
  "version": "0.0.1",
  "description": "React Boilerplate with Express (production) and Webpack Dev Server (devlopment)",
  "scripts": {
    "build": "cross-env NODE_ENV=production webpack --progress --config ./webpack/prod.js",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --config ./webpack/dev.js",
    "start": "cross-env NODE_ENV=production node ./src/server/index.js"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.5",
    "clean-webpack-plugin": "^0.1.18",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.9",
    "eslint": "^4.16.0",
    "eslint-plugin-react": "^7.6.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^2.30.1",
    "node-sass": "^4.7.2",
    "path": "^0.12.7",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-router-dom": "^4.2.2",
    "resolve-url-loader": "^2.2.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.1",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.11.1",
    "webpack-merge": "^4.1.1"
  },
  "author": "Brendan Goldberg",
  "license": "MIT"
}
